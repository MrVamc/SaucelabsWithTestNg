<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="loginSuiteSaucelabs">
	<listeners>
		<listener
			class-name="com.newframework.myListeners.myListenerClass" />
		<listener
			class-name="com.newframework.myListeners.IretryListener" />
	</listeners>

	<test name="loginTestCases">
		<classes>
			<class
				name="com.newframework.saucelabs.logintestcases.loginwithValidCredentials" />
			<class
				name="com.newframework.saucelabs.logintestcases.invalidLogin" />
			<class
				name="com.newframework.saucelabs.logintestcases.loginwithEmptyFields" />
			<class
				name="com.newframework.saucelabs.logintestcases.loginwithOnlyUsername" />
			<class
				name="com.newframework.saucelabs.logintestcases.loginwithOnlyPwd" />


		</classes>
	</test>
</suite>